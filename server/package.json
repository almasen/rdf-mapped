{
  "name": "rdfmapped.com",
  "version": "1.6.1",
  "description": "RDFmapped server",
  "main": "server.js",
  "scripts": {
    "test": "jest -i --detectOpenHandles",
    "test-nologs": "env NO_LOG=True jest -i --detectOpenHandles",
    "test-infologs": "env INFO_LOG=True jest -i --detectOpenHandles",
    "start": "node server.js",
    "db-migrate": "db-migrate",
    "docs": "jsdoc -r -c ./jsdoc/.jsdocrc.json ./routes",
    "dev-start": "env NODE_ENV=development nodemon --ignore './exports/' ./server",
    "prod-start": "env NODE_ENV=production node ./server",
    "check-all": "db-migrate reset; db-migrate up; npm i; jest -i --detectOpenHandles; jsdoc -r -c ./jsdoc/.jsdocrc.json ./routes; eslint ./ --fix",
    "rem-logs": "del-cli ./log/*.log",
    "rem-exports": "del-cli ./exports/*",
    "insert-samples": "env NO_LOG=True jest -i /test/helpers/sample"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.2.2",
    "adm-zip": "^0.4.16",
    "body-parser": "^1.19.0",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "crypto": "^1.0.1",
    "date-and-time": "^0.13.1",
    "date-fns": "^2.15.0",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "deep-equal": "^2.0.3",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "ejs-lint": "^1.1.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "express-session": "^1.17.1",
    "got": "^11.5.1",
    "helmet": "^3.23.3",
    "infinite-scroll": "^3.0.6",
    "jose": "^1.27.2",
    "jquery-ui": "^1.12.1",
    "js-base64": "^3.2.4",
    "json2csv": "^5.0.1",
    "jsonschema": "^1.2.6",
    "log4js": "^6.3.0",
    "material-design-icons": "^3.0.1",
    "node-cache": "^5.1.2",
    "node-schedule": "^1.3.2",
    "paginate-info": "^1.0.4",
    "pg": "^8.3.0",
    "pg-camelcase": "0.0.3",
    "pg-types": "^3.0.0",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "select2": "^4.0.13",
    "select2-theme-bootstrap4": "^0.2.0-beta.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.5.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^26.1.0",
    "jsdoc": "^3.6.4",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "setupFilesAfterEnv": [
      "./test/setup"
    ]
  }
}
